{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": ["eslint:recommended", "google"],
    "rules": {
        "semi": "off",
        "comma-dangle": "off",
        "no-undef": "off",
        "require-jsdoc": "off"
    }
}